bool DOS_SECCIONES_CON_IGUAL_PROFESOR(const std::vector<Section> &lista) {
    for (size_t i = 0; i < lista.size(); ++i) {
        for (size_t j = i + 1; j < lista.size(); ++j) {
            if (lista[i].professor.ci == lista[j].professor.ci) {
                return true;
            }
        }
    }
    return false;
}